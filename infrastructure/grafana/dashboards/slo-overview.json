{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus (Mimir)",
      "description": "Grafana Cloud Prometheus / Mimir — OTLP metrics land here",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana", "id": "grafana", "name": "Grafana", "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus", "version": "1.0.0" },
    { "type": "panel", "id": "stat", "name": "Stat", "version": "" },
    { "type": "panel", "id": "timeseries", "name": "Time series", "version": "" }
  ],
  "title": "TechMart — SLO Overview",
  "description": "Success rate and latency SLOs for each Critical User Journey. Import into Grafana Cloud and map DS_PROMETHEUS to your Mimir data source.",
  "uid": "techmart-slo-overview",
  "version": 1,
  "schemaVersion": 38,
  "tags": ["slo", "techmart", "cuj"],
  "time": { "from": "now-1h", "to": "now" },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "pluginId": "prometheus",
        "label": "Datasource",
        "hide": 0,
        "current": {}
      },
      {
        "name": "service",
        "type": "constant",
        "label": "Service",
        "query": "api",
        "hide": 2,
        "current": { "value": "api" }
      }
    ]
  },
  "panels": [

    { "type": "row", "title": "checkout — POST /api/orders", "id": 1,
      "collapsed": false, "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 } },

    {
      "type": "stat", "id": 2, "title": "Success Rate (1h)",
      "description": "% of checkout requests that did NOT return 5xx. SLO target: 99.9%",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(http_server_request_duration_seconds_count{http_route=\"/api/orders\",http_request_method=\"POST\",http_response_status_code!~\"5..\"}[1h])) / sum(increase(http_server_request_duration_seconds_count{http_route=\"/api/orders\",http_request_method=\"POST\"}[1h]))",
        "legendFormat": "success rate"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "orientation": "auto", "textMode": "auto", "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 95, "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red",    "value": null },
              { "color": "yellow", "value": 99   },
              { "color": "green",  "value": 99.9 }
            ]
          }
        }
      }
    },

    {
      "type": "stat", "id": 3, "title": "p99 Latency",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "1000 * histogram_quantile(0.99, sum(rate(http_server_request_duration_seconds_bucket{http_route=\"/api/orders\",http_request_method=\"POST\"}[5m])) by (le))",
        "legendFormat": "p99"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green",  "value": null },
              { "color": "yellow", "value": 500  },
              { "color": "red",    "value": 2000 }
            ]
          }
        }
      }
    },

    {
      "type": "stat", "id": 4, "title": "Request Rate",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/orders\",http_request_method=\"POST\"}[5m]))",
        "legendFormat": "req/s"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "blue" } } }
    },

    {
      "type": "timeseries", "id": 5, "title": "Error Rate vs SLO Target (0.1%)",
      "gridPos": { "x": 12, "y": 1, "w": 12, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "100 * sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/orders\",http_request_method=\"POST\",http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/orders\",http_request_method=\"POST\"}[5m]))",
          "legendFormat": "error rate %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "red",   "value": 0.1  }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "tooltip": { "mode": "single" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      }
    },

    { "type": "row", "title": "product-discovery — GET /api/products", "id": 10,
      "collapsed": false, "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 } },

    {
      "type": "stat", "id": 11, "title": "Success Rate (1h)",
      "gridPos": { "x": 0, "y": 6, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(http_server_request_duration_seconds_count{http_route=\"/api/products\",http_request_method=\"GET\",http_response_status_code!~\"5..\"}[1h])) / sum(increase(http_server_request_duration_seconds_count{http_route=\"/api/products\",http_request_method=\"GET\"}[1h]))",
        "legendFormat": "success rate"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "min": 95, "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red",    "value": null },
              { "color": "yellow", "value": 99   },
              { "color": "green",  "value": 99.9 }
            ]
          }
        }
      }
    },

    {
      "type": "stat", "id": 12, "title": "p99 Latency",
      "gridPos": { "x": 4, "y": 6, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "1000 * histogram_quantile(0.99, sum(rate(http_server_request_duration_seconds_bucket{http_route=\"/api/products\",http_request_method=\"GET\"}[5m])) by (le))",
        "legendFormat": "p99"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green",  "value": null },
              { "color": "yellow", "value": 200  },
              { "color": "red",    "value": 1000 }
            ]
          }
        }
      }
    },

    {
      "type": "stat", "id": 13, "title": "Request Rate",
      "gridPos": { "x": 8, "y": 6, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/products\",http_request_method=\"GET\"}[5m]))",
        "legendFormat": "req/s"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "blue" } } }
    },

    {
      "type": "timeseries", "id": 14, "title": "Error Rate vs SLO Target (0.1%)",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/products\",http_request_method=\"GET\",http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/products\",http_request_method=\"GET\"}[5m]))",
        "legendFormat": "error rate %"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green", "value": null },
            { "color": "red",   "value": 0.1  }
          ]}
        }
      },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    { "type": "row", "title": "order-lookup — GET /api/orders/:id", "id": 20,
      "collapsed": false, "gridPos": { "x": 0, "y": 10, "w": 24, "h": 1 } },

    {
      "type": "stat", "id": 21, "title": "Success Rate (1h)",
      "gridPos": { "x": 0, "y": 11, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(http_server_request_duration_seconds_count{http_route=\"/api/orders/:id\",http_request_method=\"GET\",http_response_status_code!~\"5..\"}[1h])) / sum(increase(http_server_request_duration_seconds_count{http_route=\"/api/orders/:id\",http_request_method=\"GET\"}[1h]))",
        "legendFormat": "success rate"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "min": 95, "max": 100,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "red",    "value": null },
            { "color": "yellow", "value": 99   },
            { "color": "green",  "value": 99.9 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 22, "title": "p99 Latency",
      "gridPos": { "x": 4, "y": 11, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "1000 * histogram_quantile(0.99, sum(rate(http_server_request_duration_seconds_bucket{http_route=\"/api/orders/:id\",http_request_method=\"GET\"}[5m])) by (le))",
        "legendFormat": "p99"
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 300  },
            { "color": "red",    "value": 1000 }
          ]}
        }
      }
    },

    {
      "type": "timeseries", "id": 23, "title": "Error Rate vs SLO Target (0.1%)",
      "gridPos": { "x": 8, "y": 11, "w": 16, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/orders/:id\",http_request_method=\"GET\",http_response_status_code=~\"5..\"}[5m])) / sum(rate(http_server_request_duration_seconds_count{http_route=\"/api/orders/:id\",http_request_method=\"GET\"}[5m]))",
        "legendFormat": "error rate %"
      }],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": { "lineWidth": 2, "fillOpacity": 10 },
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green", "value": null },
            { "color": "red",   "value": 0.1  }
          ]}
        }
      },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    }

  ]
}
