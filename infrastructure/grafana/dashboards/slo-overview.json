{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus (Mimir)",
      "description": "Grafana Cloud Prometheus / Mimir — spanmetrics land here",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana",    "id": "grafana",    "name": "Grafana",     "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus",  "version": "1.0.0"  },
    { "type": "panel",      "id": "stat",       "name": "Stat",        "version": ""       },
    { "type": "panel",      "id": "timeseries", "name": "Time series", "version": ""       }
  ],
  "title": "TechMart — SLO Overview",
  "description": "Success rate and latency SLOs per Critical User Journey, derived from spanmetrics. Metrics: techmart_calls_total / techmart_duration_milliseconds.",
  "uid": "techmart-slo-overview",
  "version": 2,
  "schemaVersion": 38,
  "tags": ["slo", "techmart", "cuj", "spanmetrics"],
  "time": { "from": "now-1h", "to": "now" },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "pluginId": "prometheus",
        "label": "Datasource",
        "hide": 0,
        "current": {}
      }
    ]
  },
  "panels": [

    { "type": "row", "title": "checkout  (POST /api/orders)", "id": 1,
      "collapsed": false, "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 } },

    {
      "type": "stat", "id": 2, "title": "Success Rate (1h)",
      "description": "Span-level success: % of cuj.checkout spans that did NOT end in STATUS_CODE_ERROR. SLO target ≥ 99.9%.",
      "gridPos": { "x": 0, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(techmart_calls_total{span_name=\"cuj.checkout\", status_code!=\"STATUS_CODE_ERROR\"}[1h])) / sum(increase(techmart_calls_total{span_name=\"cuj.checkout\"}[1h]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "decimals": 2, "min": 95, "max": 100,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "red",    "value": null  },
            { "color": "yellow", "value": 99    },
            { "color": "green",  "value": 99.9  }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 3, "title": "p99 Latency",
      "gridPos": { "x": 4, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\"}[5m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 500  },
            { "color": "red",    "value": 2000 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 4, "title": "Request Rate",
      "gridPos": { "x": 8, "y": 1, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(techmart_calls_total{span_name=\"cuj.checkout\"}[5m]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "blue" } } }
    },

    {
      "type": "timeseries", "id": 5, "title": "Error Rate vs SLO Target (0.1%)",
      "gridPos": { "x": 12, "y": 1, "w": 12, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "100 * sum(rate(techmart_calls_total{span_name=\"cuj.checkout\", status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum(rate(techmart_calls_total{span_name=\"cuj.checkout\"}[5m]))",
          "legendFormat": "error rate %"
        },
        { "expr": "vector(0.1)", "legendFormat": "SLO target (0.1%)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": [{
          "matcher": { "id": "byName", "options": "SLO target (0.1%)" },
          "properties": [
            { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } },
            { "id": "custom.lineStyle", "value": { "fill": "dash" } },
            { "id": "custom.fillOpacity", "value": 0 }
          ]
        }]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    { "type": "row", "title": "product-discovery  (GET /api/products)", "id": 10,
      "collapsed": false, "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 } },

    {
      "type": "stat", "id": 11, "title": "Success Rate (1h)",
      "gridPos": { "x": 0, "y": 6, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(techmart_calls_total{span_name=\"cuj.product-discovery\", status_code!=\"STATUS_CODE_ERROR\"}[1h])) / sum(increase(techmart_calls_total{span_name=\"cuj.product-discovery\"}[1h]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "decimals": 2, "min": 95, "max": 100,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "red",    "value": null  },
            { "color": "yellow", "value": 99    },
            { "color": "green",  "value": 99.9  }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 12, "title": "p99 Latency",
      "gridPos": { "x": 4, "y": 6, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.product-discovery\"}[5m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 200  },
            { "color": "red",    "value": 1000 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 13, "title": "Request Rate",
      "gridPos": { "x": 8, "y": 6, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(techmart_calls_total{span_name=\"cuj.product-discovery\"}[5m]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps", "color": { "mode": "fixed", "fixedColor": "blue" } } }
    },

    {
      "type": "timeseries", "id": 14, "title": "Error Rate vs SLO Target (0.1%)",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "100 * sum(rate(techmart_calls_total{span_name=\"cuj.product-discovery\", status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum(rate(techmart_calls_total{span_name=\"cuj.product-discovery\"}[5m]))",
          "legendFormat": "error rate %"
        },
        { "expr": "vector(0.1)", "legendFormat": "SLO target (0.1%)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": [{
          "matcher": { "id": "byName", "options": "SLO target (0.1%)" },
          "properties": [
            { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } },
            { "id": "custom.lineStyle", "value": { "fill": "dash" } },
            { "id": "custom.fillOpacity", "value": 0 }
          ]
        }]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    { "type": "row", "title": "order-lookup  (GET /api/orders/:id)", "id": 20,
      "collapsed": false, "gridPos": { "x": 0, "y": 10, "w": 24, "h": 1 } },

    {
      "type": "stat", "id": 21, "title": "Success Rate (1h)",
      "gridPos": { "x": 0, "y": 11, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(techmart_calls_total{span_name=\"cuj.order-lookup\", status_code!=\"STATUS_CODE_ERROR\"}[1h])) / sum(increase(techmart_calls_total{span_name=\"cuj.order-lookup\"}[1h]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "decimals": 2, "min": 95, "max": 100,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "red",    "value": null },
            { "color": "yellow", "value": 99   },
            { "color": "green",  "value": 99.9 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 22, "title": "p99 Latency",
      "gridPos": { "x": 4, "y": 11, "w": 4, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.order-lookup\"}[5m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 300  },
            { "color": "red",    "value": 1000 }
          ]}
        }
      }
    },

    {
      "type": "timeseries", "id": 23, "title": "Error Rate vs SLO Target (0.1%)",
      "gridPos": { "x": 8, "y": 11, "w": 16, "h": 4 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "100 * sum(rate(techmart_calls_total{span_name=\"cuj.order-lookup\", status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum(rate(techmart_calls_total{span_name=\"cuj.order-lookup\"}[5m]))",
          "legendFormat": "error rate %"
        },
        { "expr": "vector(0.1)", "legendFormat": "SLO target (0.1%)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": [{
          "matcher": { "id": "byName", "options": "SLO target (0.1%)" },
          "properties": [
            { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } },
            { "id": "custom.lineStyle", "value": { "fill": "dash" } },
            { "id": "custom.fillOpacity", "value": 0 }
          ]
        }]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    }

  ]
}
