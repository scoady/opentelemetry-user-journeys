{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus (Mimir)",
      "description": "Grafana Cloud Prometheus / Mimir",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana",    "id": "grafana",    "name": "Grafana",     "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus",  "version": "1.0.0"  },
    { "type": "panel",      "id": "stat",       "name": "Stat",        "version": ""       },
    { "type": "panel",      "id": "gauge",      "name": "Gauge",       "version": ""       },
    { "type": "panel",      "id": "timeseries", "name": "Time series", "version": ""       }
  ],
  "title": "TechMart â€” CUJ Deep Dive",
  "description": "Select any CUJ from the dropdown to see its full SLO health: success rate, error budget, latency percentiles, error rate, and request volume.",
  "uid": "techmart-cuj-deep-dive",
  "version": 1,
  "schemaVersion": 38,
  "tags": ["techmart", "cuj", "slo", "deep-dive"],
  "time": { "from": "now-1h", "to": "now" },
  "refresh": "30s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "pluginId": "prometheus",
        "label": "Datasource",
        "hide": 0,
        "current": {}
      },
      {
        "name": "cuj",
        "type": "custom",
        "label": "CUJ",
        "query": "checkout,product-discovery,product-search,product-review,order-lookup,order-history,product-upload,product-upload-job",
        "options": [
          { "text": "checkout",           "value": "checkout",           "selected": true  },
          { "text": "product-discovery",  "value": "product-discovery",  "selected": false },
          { "text": "product-search",     "value": "product-search",     "selected": false },
          { "text": "product-review",     "value": "product-review",     "selected": false },
          { "text": "order-lookup",       "value": "order-lookup",       "selected": false },
          { "text": "order-history",      "value": "order-history",      "selected": false },
          { "text": "product-upload",     "value": "product-upload",     "selected": false },
          { "text": "product-upload-job", "value": "product-upload-job", "selected": false }
        ],
        "current": { "text": "checkout", "value": "checkout" },
        "hide": 0
      },
      {
        "name": "window",
        "type": "custom",
        "label": "SLO Window",
        "query": "1h,6h,24h,7d,30d",
        "options": [
          { "text": "1h",  "value": "1h",  "selected": false },
          { "text": "6h",  "value": "6h",  "selected": false },
          { "text": "24h", "value": "24h", "selected": true  },
          { "text": "7d",  "value": "7d",  "selected": false },
          { "text": "30d", "value": "30d", "selected": false }
        ],
        "current": { "text": "24h", "value": "24h" },
        "hide": 0
      }
    ]
  },
  "panels": [

    {
      "type": "stat", "id": 1, "title": "Success Rate",
      "description": "Percentage of spans completing without error over the selected window.",
      "gridPos": { "x": 0, "y": 0, "w": 5, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(increase(techmart_calls_total{span_name=\"cuj.${cuj}\", status_code!=\"STATUS_CODE_ERROR\"}[${window}])) / sum(increase(techmart_calls_total{span_name=\"cuj.${cuj}\"}[${window}]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "none" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "decimals": 3, "min": 99, "max": 100,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "red",    "value": null },
            { "color": "yellow", "value": 99   },
            { "color": "green",  "value": 99.9 }
          ]}
        }
      }
    },

    {
      "type": "gauge", "id": 2, "title": "Error Budget Remaining",
      "description": "How much of the 30-day 0.1% error budget is left.",
      "gridPos": { "x": 5, "y": 0, "w": 5, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "clamp_min(100 * (1 - (sum(increase(techmart_calls_total{span_name=\"cuj.${cuj}\", status_code=\"STATUS_CODE_ERROR\"}[30d])) / sum(increase(techmart_calls_total{span_name=\"cuj.${cuj}\"}[30d]))) / 0.001), 0)",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "showThresholdLabels": true, "showThresholdMarkers": true },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "decimals": 1, "min": 0, "max": 100,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "red",    "value": null },
            { "color": "yellow", "value": 25   },
            { "color": "green",  "value": 50   }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 3, "title": "p99 Latency",
      "description": "99th percentile latency for the selected CUJ.",
      "gridPos": { "x": 10, "y": 0, "w": 4, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.${cuj}\"}[5m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 500  },
            { "color": "red",    "value": 2000 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 4, "title": "Throughput",
      "description": "Current request rate (requests per second).",
      "gridPos": { "x": 14, "y": 0, "w": 4, "h": 2 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(techmart_calls_total{span_name=\"cuj.${cuj}\"}[5m]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },

    {
      "type": "stat", "id": 5, "title": "Errors (window)",
      "description": "Total error count over the selected window.",
      "gridPos": { "x": 14, "y": 2, "w": 4, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(increase(techmart_calls_total{span_name=\"cuj.${cuj}\", status_code=\"STATUS_CODE_ERROR\"}[${window}]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green", "value": null },
            { "color": "red",   "value": 1    }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 6, "title": "p50 Latency",
      "gridPos": { "x": 18, "y": 0, "w": 3, "h": 2 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.50, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.${cuj}\"}[5m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "ms" } }
    },

    {
      "type": "stat", "id": 7, "title": "p95 Latency",
      "gridPos": { "x": 18, "y": 2, "w": 3, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.95, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.${cuj}\"}[5m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "ms" } }
    },

    {
      "type": "stat", "id": 8, "title": "Total Requests",
      "gridPos": { "x": 21, "y": 0, "w": 3, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(increase(techmart_calls_total{span_name=\"cuj.${cuj}\"}[${window}]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },

    {
      "type": "timeseries", "id": 10, "title": "Error Rate vs SLO Target",
      "description": "Dashed red line = 0.1% SLO budget. Any time the solid line crosses it, error budget is burning.",
      "gridPos": { "x": 0, "y": 5, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "100 * sum(rate(techmart_calls_total{span_name=\"cuj.${cuj}\", status_code=\"STATUS_CODE_ERROR\"}[5m])) / sum(rate(techmart_calls_total{span_name=\"cuj.${cuj}\"}[5m]))",
          "legendFormat": "error rate"
        },
        { "expr": "vector(0.1)", "legendFormat": "SLO target (0.1%)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "custom": { "lineWidth": 2, "fillOpacity": 10 } },
        "overrides": [{
          "matcher": { "id": "byName", "options": "SLO target (0.1%)" },
          "properties": [
            { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } },
            { "id": "custom.lineStyle", "value": { "fill": "dash" } },
            { "id": "custom.fillOpacity", "value": 0 }
          ]
        }]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    {
      "type": "timeseries", "id": 11, "title": "Latency Percentiles",
      "description": "p50 / p95 / p99 over time for the selected CUJ.",
      "gridPos": { "x": 12, "y": 5, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.${cuj}\"}[5m])) by (le))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.${cuj}\"}[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.${cuj}\"}[5m])) by (le))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ms", "custom": { "lineWidth": 2, "fillOpacity": 0 } }
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    {
      "type": "timeseries", "id": 12, "title": "Request Rate",
      "description": "Requests per second for the selected CUJ.",
      "gridPos": { "x": 0, "y": 13, "w": 12, "h": 7 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(techmart_calls_total{span_name=\"cuj.${cuj}\"}[5m]))",
        "legendFormat": "requests/sec"
      }],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "lineWidth": 2, "fillOpacity": 15 } }
      },
      "options": { "tooltip": { "mode": "single" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    {
      "type": "timeseries", "id": 13, "title": "Request Volume by Status",
      "description": "Stacked area showing successful vs failed requests.",
      "gridPos": { "x": 12, "y": 13, "w": 12, "h": 7 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum by (status_code) (rate(techmart_calls_total{span_name=\"cuj.${cuj}\"}[5m]))",
        "legendFormat": "{{status_code}}"
      }],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "custom": { "lineWidth": 1, "fillOpacity": 20, "stacking": { "mode": "normal" } } },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "STATUS_CODE_ERROR" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
          },
          {
            "matcher": { "id": "byName", "options": "STATUS_CODE_OK" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
          },
          {
            "matcher": { "id": "byName", "options": "STATUS_CODE_UNSET" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }]
          }
        ]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    }

  ]
}
