{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus (Mimir)",
      "description": "Grafana Cloud Prometheus / Mimir",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    { "type": "grafana",    "id": "grafana",    "name": "Grafana",     "version": "10.0.0" },
    { "type": "datasource", "id": "prometheus", "name": "Prometheus",  "version": "1.0.0"  },
    { "type": "panel",      "id": "stat",       "name": "Stat",        "version": ""       },
    { "type": "panel",      "id": "timeseries", "name": "Time series", "version": ""       },
    { "type": "panel",      "id": "bargauge",   "name": "Bar gauge",   "version": ""       }
  ],
  "title": "TechMart — Checkout CUJ Latency Breakdown",
  "description": "Per-service p99 latency breakdown for the checkout CUJ. Shows where time is spent across api and inventory-svc. Adjust inventorySvc.artificialDelayMs via helm upgrade to see the attribution shift in real time.",
  "uid": "techmart-checkout-breakdown",
  "version": 1,
  "schemaVersion": 38,
  "tags": ["slo", "techmart", "checkout", "cuj", "breakdown", "latency"],
  "time": { "from": "now-30m", "to": "now" },
  "refresh": "15s",
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "pluginId": "prometheus",
        "label": "Datasource",
        "hide": 0,
        "current": {}
      }
    ]
  },
  "panels": [

    {
      "type": "stat", "id": 1,
      "title": "Checkout p99 (total)",
      "description": "End-to-end p99 latency of the cuj.checkout span in the API — includes all downstream hops (inventory, DB).",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms", "decimals": 0,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 500  },
            { "color": "red",    "value": 2000 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 2,
      "title": "inventory-svc p99",
      "description": "p99 of the POST /reserve span inside inventory-svc. Labelled cuj_name=checkout by the W3C Baggage middleware. This is the dominant latency contributor when artificialDelayMs > 0.",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background", "graphMode": "area" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms", "decimals": 0,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 400  },
            { "color": "red",    "value": 1500 }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 3,
      "title": "API overhead p99",
      "description": "Approximate API-only overhead: checkout total minus inventory-svc latency. Reflects DB queries, validation, and serialisation cost in the api service itself.",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le)) - ignoring() histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value", "graphMode": "area" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms", "decimals": 0,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 200  },
            { "color": "red",    "value": 500  }
          ]}
        }
      }
    },

    {
      "type": "stat", "id": 4,
      "title": "Checkout RPS",
      "description": "Current checkout request rate.",
      "gridPos": { "x": 18, "y": 0, "w": 3, "h": 2 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(techmart_calls_total{span_name=\"cuj.checkout\", service_name=\"api\"}[2m]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },

    {
      "type": "stat", "id": 5,
      "title": "inventory-svc RPS",
      "description": "Current reservation request rate.",
      "gridPos": { "x": 18, "y": 2, "w": 3, "h": 3 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "sum(rate(techmart_calls_total{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "value" },
      "fieldConfig": { "defaults": { "unit": "reqps" } }
    },

    {
      "type": "stat", "id": 6,
      "title": "inventory-svc error rate",
      "description": "Any non-zero value here means reservations are failing and the checkout CUJ will error.",
      "gridPos": { "x": 21, "y": 0, "w": 3, "h": 5 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [{
        "expr": "100 * sum(rate(techmart_calls_total{service_name=\"inventory-svc\", cuj_name=\"checkout\", status_code=\"STATUS_CODE_ERROR\"}[2m])) / sum(rate(techmart_calls_total{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m]))",
        "legendFormat": ""
      }],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] }, "colorMode": "background" },
      "fieldConfig": {
        "defaults": {
          "unit": "percent", "decimals": 2,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green", "value": null },
            { "color": "red",   "value": 0.1  }
          ]}
        }
      }
    },

    {
      "type": "timeseries", "id": 10,
      "title": "p99 Latency by Service",
      "description": "Where checkout time is spent. The gap between 'checkout total' and 'inventory-svc' lines is the pure API overhead. Tip: change inventorySvc.artificialDelayMs and watch the attribution shift.",
      "gridPos": { "x": 0, "y": 5, "w": 16, "h": 9 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le))",
          "legendFormat": "checkout total (api)"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "inventory-svc  ← artificial delay"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le)) - ignoring() histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "api overhead"
        },
        { "expr": "vector(2000)", "legendFormat": "SLO target (2 s)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ms", "custom": { "lineWidth": 2, "fillOpacity": 5 } },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "inventory-svc  \u2190 artificial delay" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }]
          },
          {
            "matcher": { "id": "byName", "options": "checkout total (api)" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "blue"   } }]
          },
          {
            "matcher": { "id": "byName", "options": "api overhead" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green"  } },
              { "id": "custom.lineStyle", "value": { "fill": "dash" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "SLO target (2 s)" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red"    } },
              { "id": "custom.lineStyle", "value": { "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 }
            ]
          }
        ]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },

    {
      "type": "bargauge", "id": 11,
      "title": "p99 Contribution by Service",
      "description": "Current p99 latency for each service in the checkout call chain. The longest bar is the bottleneck.",
      "gridPos": { "x": 16, "y": 5, "w": 8, "h": 9 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le))",
          "legendFormat": "checkout total"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "inventory-svc"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le)) - ignoring() histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "api overhead"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"] },
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms", "min": 0, "max": 2000,
          "thresholds": { "mode": "absolute", "steps": [
            { "color": "green",  "value": null },
            { "color": "yellow", "value": 500  },
            { "color": "red",    "value": 1500 }
          ]}
        }
      }
    },

    {
      "type": "timeseries", "id": 12,
      "title": "p50 / p95 / p99 — checkout total vs inventory-svc",
      "description": "Full latency percentile picture for both services. Useful for seeing whether high p99 is a tail issue or a consistent problem.",
      "gridPos": { "x": 0, "y": 14, "w": 24, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le))",
          "legendFormat": "checkout p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le))",
          "legendFormat": "checkout p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{span_name=\"cuj.checkout\", service_name=\"api\"}[2m])) by (le))",
          "legendFormat": "checkout p99"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "inventory-svc p50"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "inventory-svc p95"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(techmart_duration_milliseconds_bucket{service_name=\"inventory-svc\", cuj_name=\"checkout\"}[2m])) by (le))",
          "legendFormat": "inventory-svc p99"
        },
        { "expr": "vector(2000)", "legendFormat": "SLO target (2 s)" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ms", "custom": { "lineWidth": 1, "fillOpacity": 0 } },
        "overrides": [
          {
            "matcher": { "id": "byRegexp", "options": "inventory-svc.*" },
            "properties": [{ "id": "color", "value": { "mode": "palette-classic-by-name" } }]
          },
          {
            "matcher": { "id": "byName", "options": "SLO target (2 s)" },
            "properties": [
              { "id": "color",              "value": { "mode": "fixed", "fixedColor": "red" } },
              { "id": "custom.lineStyle",   "value": { "fill": "dash" } },
              { "id": "custom.fillOpacity", "value": 0 },
              { "id": "custom.lineWidth",   "value": 1 }
            ]
          }
        ]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    }

  ]
}
